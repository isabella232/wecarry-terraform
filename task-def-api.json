[
  {
    "dnsSearchDomains": null,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "${log_stream_prefix}"
      }
    },
    "entryPoint": null,
    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "command": null,
    "linuxParameters": null,
    "cpu": ${cpu},
    "environment": [
      {
        "name": "DATABASE_URL",
        "value": "${DATABASE_URL}"
      },
      {
        "name": "ADDR",
        "value": "0.0.0.0"
      },
      {
        "name": "PORT",
        "value": "3000"
      },
      {
        "name": "HOST",
        "value": "${HOST}"
      },
      {
        "name": "UI_URL",
        "value": "${UI_URL}"
      },
      {
        "name": "GO_ENV",
        "value": "production"
      },
      {
        "name": "SAML_IDP_SSO_URL",
        "value": "${saml_idp_sso_url}"
      },
      {
        "name": "SAML_IDP_ENTITY_ID",
        "value": "${saml_idp_entity_id}"
      },
      {
        "name": "SAML_IDP_CERT_DATA",
        "value": "${saml_idp_cert_data}"
      }
    ],
    "ulimits": null,
    "dnsServers": null,
    "mountPoints": [],
    "workingDirectory": null,
    "dockerSecurityOptions": null,
    "memory": ${memory},
    "memoryReservation": null,
    "volumesFrom": [],
    "image": "${docker_image}:${docker_tag}",
    "disableNetworking": null,
    "healthCheck": null,
    "essential": true,
    "links": null,
    "hostname": null,
    "extraHosts": null,
    "user": null,
    "readonlyRootFilesystem": null,
    "dockerLabels": null,
    "privileged": null,
    "name": "buffalo"
  }
]
